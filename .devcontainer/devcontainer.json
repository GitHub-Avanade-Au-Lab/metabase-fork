{
  "name": "Node.js",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "extensions": [
    "github.copilot",
    "github.copilot-chat"
  ],
  "forwardPorts": [3000],
  "postCreateCommand": "yarn install && clojure -M:dev",
  "initializeCommand": "docker network create metabase-dev || true",
  "remoteUser": "vscode",
  "runArgs": ["--network=metabase-dev"],
  "postStartCommand": "git config --global --add safe.directory /workspaces/metabase"
}
